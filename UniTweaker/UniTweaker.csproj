    <ItemGroup>
        <Folder Include="UI\" />
    </ItemGroup>
    
    <ItemGroup>
        <ProjectReference Include="..\UniTweaker.UI\UniTweaker.UI.csproj" />
    </ItemGroup>
</Project>
